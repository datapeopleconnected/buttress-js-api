{
  "name": "buttress-js-api",
  "description": "ButtressJS API for Node.js",
  "version": "2.4.0",
  "author": "Lighten",
  "contributors": [
    "Chris Bates-Keegan <chris@wearelighten.co.uk>"
  ],
  "homepage": "https://github.com/wearelighten/buttress-js-api",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearelighten/buttress-js-api"
  },
  "bugs": {
    "url": "https://github.com/wearelighten/buttress-js-api/issues"
  },
  "engines": {
    "node": "^10.15.0"
  },
  "scripts": {
    "lint": "eslint 'lib/**/*.js'",
    "test": "mocha --require should --reporter spec",
    "test-app": "mocha --require should --reporter spec -g app-basics",
    "test-company": "mocha --require should --reporter spec -g company",
    "test-service": "mocha --require should --reporter spec -g service",
    "test-posts": "mocha --require should --reporter spec -g @posts",
    "test-users": "mocha --require should --reporter spec -g @users",
    "test-roles": "mocha --require should --reporter spec -g @roles",
    "test-boards": "mocha --require should --reporter spec -g @boards",
    "test-data-filter": "mocha --require should --reporter spec -g @data-filter",
    "test-socket": "mocha --require should --reporter spec -g @socket"
  },
  "main": "./lib/buttressjs",
  "dependencies": {
    "axios": "^0.19.2",
    "mongodb": "^3.6.2",
    "sugar": "^2.0.6"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.1.3",
    "should": "^13.2.3"
  }
}
